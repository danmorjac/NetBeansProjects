<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
        <servlet>
        <servlet-name>ServletCicloVida</servlet-name>
        <servlet-class>Servlets.ServletCicloVida</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletPeticiones</servlet-name>
        <servlet-class>Servlets.ServletPeticiones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrimerServlet</servlet-name>
        <servlet-class>Servlets.PrimerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletScope1</servlet-name>
        <servlet-class>Servlets.ServletScope1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletScop2</servlet-name>
        <servlet-class>Servlets.ServletScop2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletScope2</servlet-name>
        <servlet-class>Servlets.ServletScope2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletScope3</servlet-name>
        <servlet-class>Servlets.ServletScope3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletCicloVida</servlet-name>
        <url-pattern>/ServletCicloVida</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletPeticiones</servlet-name>
        <url-pattern>/ServletPeticiones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrimerServlet</servlet-name>
        <url-pattern>/PrimerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletScope1</servlet-name>
        <url-pattern>/ServletScope1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletScop2</servlet-name>
        <url-pattern>/ServletScop2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletScope2</servlet-name>
        <url-pattern>/ServletScope2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletScope3</servlet-name>
        <url-pattern>/ServletScope3</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list>
    </web-app>
